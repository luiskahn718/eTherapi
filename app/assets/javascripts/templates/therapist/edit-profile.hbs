                          {{/each}}
                        </select>
                      </div>
                      <div class="popover right">
                        <div class="arrow"></div>
                        <div class="pop-over-content">Click to select among the options provided.</div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="treatment form-group">
              <div class="row no-gutter">
                <div class="col-md-6">
                  <div class="title">Treatment Approach</div>
                  <div class="treatment-approach form-group">
                    <div class="include-popover col-md-12 no-gutter">
                      <!--define the select-chosen-multiple-->
                      <div class="chosen-wrapper col-md-5 no-gutter">
                        <div class="tagsinput col-md-9">
                          <input type="text" value="{{therapist_profile_attributes.treatment_approach}}" class="input-tags treatment-tags" id="treatment-txt" placeholder="Type"/>
                        </div>
                      </div>
                      <div class="popover right">
                        <div class="arrow"></div>
                        <div class="pop-over-content">Click to select among the options provided or enter additional ones.</div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="title">Demographics Served</div>
                  <div class="demographic-served form-group">
                    <div class="include-popover col-md-12 no-gutter">
                      <!--define the select-chosen-multiple-->
                      <div class="chosen-wrapper col-md-5 no-gutter">
                        <div class="tagsinput col-md-9">
                          <input type="text" value="{{therapist_profile_attributes.demographics_served}}" class="input-tags demographics-served" id="demographics-txt" placeholder="Type"/>
                        </div>
                      </div>
                      <div class="popover right">
                        <div class="arrow"></div>
                        <div class="pop-over-content">Click to select among the options provided or enter additional ones.</div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </form>
        </div>
