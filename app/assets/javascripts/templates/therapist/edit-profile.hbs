
<div class="therapist-edit-profile">
  <!--degine the content-header of the edit-profile page-->
  <div class="content-header">
    <div class="header-section">
      <h1>Public Profile<i class="icon-user8"></i></h1>
      <br /><small>The information on this page will be visible to users searching for therapists or psychiatrists on the eTherapi website.</small>
    </div>
  </div>
  <!--define the content of edit-profile page-->
  <div class="row">
    <div class="edit-profile-nav slave-nav col-md-3 affix-menu-nav aside-content">
      <!--define the list-group-->
      <ul class="list-group nav">
        <li target=".profile-pic-section">
          <a href="#profile-pic-section" class="list-group-item profile-pic-nav profile-pic-section"><h4 class="list-group-item-heading">Profile Picture and Video</h4></a>
        </li>
        <li target=".about-section">
          <a href="#about-section" class="list-group-item about-nav"><h4 class="list-group-item-heading about-section">About</h4></a>
        </li>
        <li target=".finances-section">
          <a href="#finances-section" class="list-group-item finances-nav"><h4 class="list-group-item-heading finances-section">Finances</h4></a>
        </li>
        <li target=".specialties-section">
          <a href="#specialties-section" class="list-group-item specialties-nav"><h4 class="list-group-item-heading specialties-section">Specialties and Expertise</h4></a>
        </li>
        <li target=".credentials-section">
          <a href="#credentials-section" class="list-group-item credentials-nav"><h4 class="list-group-item-heading credentials-section">Credentials</h4></a>
        </li>
      </ul>
    </div>

    <div class="edit-profile-content listview-content col-md-9 pri-content">
      
      <div class="profile-button-group">
        <a href="javascript:void(0);" class="btn btn-success view-profile-btn">View Profile</a>
        <button type="button" class="btn btn-info save-edit-profile">Save Profile</button>
      </div>

      <div class="edit-profile-detail">
        <!--define the picture-video block-->
        <div class="picture-video block col-md-12" id="profile-pic-section">
          <div class="block-title">
            <h2>Profile Picture and Video</h2>
          </div>
          <form method="post" class="form-horizontal form-bordered">
            <div class="avatar-container row no-gutter">
              <div class="col-md-6 no-gutter">
                <div class="user-avatar-section clearfix">
                  <div class="user-avatar">
                    <a href="javascript:void(0)" class="img-rounded">
                      <img src="{{checkAvatar120 picture_filelink.url gender}}?{{currentTime}}" alt="avatar" id="real-avt"/>
                    </a>
                  </div>
                  <div class="avatar-action">
                    <div class="user-avatar-upload">
                      <button data-toggle="modal" data-target="#profile-upload-modal" type="button" class="btn btn-info change-avatar">Upload New Picture</button>
                    </div>
                    <div class="user-avatar-delete"><a href="javascript:void(0)" class="text-danger" id="del-avatar-btn">Delete picture</a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </form>
          <form method="post" class="introduction-video form-horizontal form-bordered">
            <div class="title">Introduction Video</div>
            <div class="record-video row no-gutter">
              <div class="col-md-12 no-gutter">
                <a href="https://www.youtube.com/my_webcam" target="_blank" class="btn btn-info record-btn">Record an introduction video using YouTube</a>
              </div>
              <div class="col-md-12 no-gutter">
                <label class="control-label">Enter the link to your introduction YouTube video.</label>
                <div class="input-group">
                  <input type="text" placeholder="e.g., https://www.youtube.com/watch?v=AopXAyul7yY" class="form-control medium-form-control" value="{{therapist_profile_attributes.youtube_link}}" id="youtube-url"/><span class="input-group-addon group-addon-lg"><i class="fa fa-youtube-play"></i></span>
                </div>
                <div class="help-block animation-slideDown">Please enter a valid Youtube URL!</div>
              </div>
            </div>
          </form>
        </div>
        <!--define the about block-->
        <div class="about block col-md-12" id="about-section">
          <div class="block-title">
            <h2>About</h2>
          </div>
          <form method="post" class="form-horizontal form-bordered">
            <div class="title">Bio</div>
            <div class="form-group">
              <div class="editor-group col-xs-12">
                <textarea rows="10" class="form-control textarea-editor" value="{{therapist_profile_attributes.about_me}}" id="about-me-txt"></textarea>
              </div>
            </div>
            <div class="title language-title">Languages Spoken</div>
            <div class="form-group">
              <div class="include-popover language-spoken col-md-12">
                <!--define the select-chosen-multiple -->
                <div class="chosen-wrapper col-md-6 no-gutter">
                  <select data-placeholder="Select" class="form-control input-md" id="language-spoken-txt" multiple>
                    {{#each language_cdes}}
                      <option value="{{language}}--{{language_code}}" {{checkMulLang language_code ../therapist_language_attributes}}>{{language}}</option>
                    {{/each}}
                  </select>
                  <div class="popover right">
                    <div class="arrow"></div>
                    <div class="pop-over-content">Click to select among the options provided.</div>
                  </div>
                </div>
              </div>
            </div>
          </form>
        </div>
        <!--define the price block-->
        <div class="price block col-md-12" id="finances-section">
          <div class="block-title">
            <h2>Finances</h2>
          </div>
          <form method="post" class="form-horizontal form-bordered">
            <div class="title">Pricing</div>
            <!--form-group include select cash and time-->
            <div class="pricing form-group">
              <div class="col-md-7">
                <div class="cash col-md-5">
                  <div class="input-group">
                    <div class="input-group-addon"><i class="fa fa-usd"></i>
                    </div>
                    <input type="text" class="form-control valid medium-form-control" value="{{therapist_profile_attributes.hourly_rate_max}}" id="pricing-txt"/>
                  </div>
                  <div class="help-block animation-slideDown">Fee must be an integer number!</div>
                </div>
                <div class="solid-text col-md-2">for up to</div>
                <div class="time col-md-5">
                  <select class="select-chosen form-control" id="duration-chosen">
                    <option value="0" {{checkSelectedChosen session_duration 0}}>0 minute</option>
                    <option value="15" {{checkSelectedChosen session_duration 15}}>15 minutes</option>
                    <option value="30" {{checkSelectedChosen session_duration 30}}>30 minutes</option>
                    <option value="45" {{checkSelectedChosen session_duration 45}}>45 minutes</option>
                    <option value="60" {{checkSelectedChosen session_duration 60}}>60 minutes</option>
                    <option value="75" {{checkSelectedChosen session_duration 75}}>75 minutes</option>
                    <option value="90" {{checkSelectedChosen session_duration 90}}>90 minutes</option>
                  </select>
                </div>
              </div>
              <div class="pricing-offer col-md-7">
                <div class="checkbox">
                  <label>
                    <input type="checkbox" {{checkCheckbox therapist_profile_attributes.sliding_scale}} id="sliding-scale-cb"/><span>I offer a sliding scale - Let patients contact me for details</span>
                  </label>
                </div>
              </div>
            </div>
            <!--subtitle for block-->
            <div class="title cancellation-title">Cancellation</div>
            <!--form-group include checkbox and popover-->
            <div class="cancellation include-popover form-group">
              <div class="col-md-12">
                <div class="checkbox">
                  <label>
                    <input type="checkbox" {{checkCheckbox therapist_profile_attributes.charge_for_cancellation}} id="cancel-cb"/><span>Charge a cancellation fee if a client cancels with less than 24 hours notice</span>
                  </label>
                  <div class="popover right">
                    <div class="arrow"></div>
                    <div class="pop-over-content">You will be given the option to charge the client up to the session amount after the late cancellation occurs.</div>
                  </div>
                </div>
              </div>
            </div>
            <div class="title">Insurance Plans Accepted</div>
            <div class="insurance-plan form-group">
              <div class="include-popover col-md-12">
                <!--define the select-chosen-multiple-->
                <div class="chosen-wrapper col-md-6 no-gutter">
                  <select data-placeholder="Add Insurance" class="form-control input-md" id="insurance-txt" multiple>
                    {{#each insurance_payers_name}}
                      <option value="{{payers_name}}--{{id}}" {{checkMulInsurances id ../therapist_accept_insurance_attributes}}>{{payers_name}}</option>
                    {{/each}}
                  </select>
                  <div class="popover right">
                    <div class="arrow"></div>
                    <div class="pop-over-content">Click to select among the options provided.</div>
                  </div>
                </div>
              </div>
            </div>
          </form>
        </div>
        <!--define the Expertise and Specialties block-->
        <div class="expertise block col-md-12" id="specialties-section">
          <div class="block-title">
            <h2>Specialties and Expertise</h2>
          </div>
          <form method="post" class="specialties-form form-horizontal form-bordered">
            <div class="form-group">
              <div class="row no-gutter">
                <div class="col-md-6">
                  <div class="title">Specialties (up to 3)</div>
                  <div class="specialties form-group">
                    <div class="include-popover col-md-12 no-gutter">
                      <!--define the select-chosen-multiple-->
                      <div class="chosen-wrapper col-md-5 no-gutter speciality">
                        <select data-placeholder="Select" class="form-control input-md" id="speciality-txt" multiple>
                          <option></option>
                          {{#each speciality}}
                            <option value="{{name}}-{{speciality_id}}" {{checkMulChosen speciality_id ../specialityArr}}>{{name}}</option>
                          {{/each}}
                        </select>
                      </div>
                      <div class="popover right">
                        <div class="arrow"></div>
                        <div class="pop-over-content">Click to select among the options provided.</div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="title">Additional Expertise</div>
                  <div class="additional-expertise form-group">
                    <div class="include-popover col-md-12 no-gutter">
                      <!--define the select-chosen-multiple-->
                      <div class="chosen-wrapper col-md-5 no-gutter">
                        <select data-placeholder="Select" class="form-control input-md" id="additional-txt" multiple>
                          {{#each speciality}}
